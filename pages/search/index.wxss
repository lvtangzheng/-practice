.container{
	overflow: auto;
	position: relative;
}
.topFix{
	width: 100%;
	position: fixed;
	z-index: 9999;
}
form{
	display: block;
}
/*搜索框*/
.search-wrap{
	width: 750rpx;
	padding-bottom: 30rpx ;
	background: #fff;
}
.search-box{
	width: 620rpx;
	margin-left: 20rpx;
	background: #f2f2f2;
	border-radius: 30rpx;
	height: 60rpx;
	position: relative;
}
.search-box2{
	width: 95%;
	margin-left: 20rpx;
	background: #f2f2f2;
	border-radius: 30rpx;
	height: 60rpx;
	position: relative;
}
.search-box input{
	display: inline-block;
	height: 60rpx;
	line-height: 60rpx;
	font-size: 28rpx;
	padding-left: 64rpx;
	width: 85%;
}
.search-wrap .search-box image{
	display: inline-block;
	width: 30rpx;
	height: 30rpx;
	position: absolute;
	top: 50%;
	left: 20rpx;
	transform: translateY(-50%);
}
.search-wrap .search-box .search-btn{
	color: #666;
	font-size: 26rpx;
	float: right;
	position: absolute;
	top: 0rpx;
	right: 0;
	line-height:1;
	padding: 16rpx;
	background: transparent;
}
.search-wrap .search-box .search-btn2{
	font-size: 30rpx;
	color: #555;
	right: -100rpx;
}

.content-wrap{
	padding: 0 30rpx;
	background: #fff;
	padding-bottom: 0rpx;
	z-index: 9999;
	display: flex;
	border-bottom: 1px solid #eee;
	border-top: 1px solid #eee;
}
.content-wrap .flex-item{
	flex: 1;
	display: flex;
	justify-content: center;
	align-items: center;
	font-size: 26rpx;
	padding: 30rpx 0;
	color: #666;
	line-height: 1;
}
.content-wrap .flex-item text{
	overflow:hidden;
	text-overflow:ellipsis;
	white-space:nowrap;
	display:inline-block;
	max-width:150rpx;
}
.content-wrap .flex-item image{
	width: 14rpx;
	height: 8rpx;
	padding-left: 5rpx;
}
.content-wrap .flex-item image.imgset{
	width: 16rpx;
	height: 18rpx;
	padding-left: 5rpx;
}
.content-wrap .flex-item.active{
	color: #ff7600;
}
.search-none{
	padding-top:175rpx; 
	flex-direction: column;
	align-items: center;
}
.search-none .search-info p{
	padding-top:20rpx;
	display: block;
	text-align: center;
	font-size:30rpx;
	color:#999;
}
.search-none button{
	margin-top: 60rpx;
	text-align: center;
	padding: 15rpx 0;
	background: #F60;
	border-radius: 6px;
	color: #fff;
	border: none;
	font-size: 30rpx;
	line-height: 1;
	width: 270rpx;
}


/*不限规格下拉筛选*/
.modalContent{
	position: fixed;
	top: 170rpx;
	bottom: 0;
	overflow: auto;
	width: 750rpx;
	z-index: 9;
	background: rgba(25,25,25,.2);
}
.modalBg{
	background: #f0f6f7;
	padding-top: 20rpx;
}
.modal-mask{
	position:fixed;
	top:0;
	left:0;
	width:100%;
	height:100%;
	z-index:-5;
}
.modalContent .modal{
	background: #fff;
	font-size: 28rpx;
	color: #333;
	z-index:99;
	padding: 25rpx 0 30rpx;
}
.modalContent .modal .all{
	width:150rpx;
	line-height:60rpx;
	text-align:center;
	color: #555;
	border-radius:6rpx;
	border: 1px solid #d0d0d0;
	margin:0 0 30rpx 25rpx;
}
.modalContent .modal .craftActive{
	color: #ff7600;
	background:#fff8f2;
	border: 1px solid #ff7600!important;
}


.modalContent .modal .modal-footer{
	position: relative;
	overflow: hidden;
}
.modalTitle{
	font-size: 28rpx;
	color: #333;
	line-height: 1;
	padding-bottom: 20rpx;
}
.modalTitle text{
	font-size: 26rpx;
	color: #787878;
}

.modalContent .modal .modal-footer .wrapSet{
	border-top: 1px solid #eee;
	border-bottom: 1px solid #eee;
	position: relative;
	overflow: hidden;
	padding: 20rpx 30rpx 30rpx;
}
.modalContent .modal .modal-footer .wrapSet .item-input{
	float: left;
	width: 200rpx;
	background: #f2f2f2;
	border:2rpx solid #f2f2f2;
	border-radius:6rpx;
	position: relative;
	height: 58rpx;
	margin-right: 20rpx;
}

.modalContent .modal .modal-footer .wrapSet .item-input input{
	padding: 2rpx 0 2px;
	width:140rpx;
	text-align:center;
	background:#fff;
	color: #f60;
	box-sizing:border-box;
	position: absolute;
	top: 50%;
	transform: translateY(-50%);
	height: 100%;
}
.modalContent .modal .modal-footer .wrapSet .item-input view{
	position: absolute;
	top: 50%;
	transform: translateY(-50%);
	right: 0;
	width: 64rpx;
	text-align: center;
	line-height: 1;
}
.modalContent .modal .modal-footer .btnSure{
	width:200rpx;
	height:60rpx;
	border-radius: 6rpx;
	text-align: center;
	background: #ff7600;
	color: #fff;
	border: 1rpx solid #f60;
	font-size: 30rpx;
	line-height: 60rpx;
	height: 60rpx;
	margin-top: 40rpx;
}

.modalContent .modal .arr-up image{
	width: 24rpx;
	height: 12rpx;
	display: block;
	margin: 20rpx auto 0; 
}

/*列表数据*/
.item_wrap{
	background: #fff;
	width: 750rpx;
	margin-top: 190rpx;
}
.item_wrap .item{
	padding: 22rpx 30rpx 30rpx 30rpx;
	border-bottom: 1px solid #eee;
	position: relative;
	box-sizing: border-box;
}
.item_wrap .item .recommend{
	position: absolute;
	right: 0;
	top: 0;
	width: 65rpx;
	height: 65rpx;
}
.item_flex{
	display: flex;
	line-height: 1;
	position: relative;
	margin-bottom: 20rpx;
}
.item_flex:last-child{
	margin-bottom: 0;
}
.item_flex .item_left{
	width: 490rpx;
	display: flex;
	align-items: center;
}
.item_flex .item_right{
	width: 200rpx;
	text-align: right;
}
.item_flex .item_left .sku_label{
	width: 28rpx;
	height: 28rpx;
	line-height: 28rpx;
	text-align: center;
	background: #ef7560;
	color: #fff;
	font-size: 24rpx;
	border-radius: 4rpx;
	position: absolute;
	top: 10rpx;
}
.item_flex .item_left .sku_label2{
	background: #ffbb62;
}
.item_flex .item_left .title{
	font-size: 32rpx; 
	color: #333;
	line-height: 46rpx;
	position: relative;
	text-indent: 40rpx;
}
.item_flex .item_left .desc{
	overflow: hidden;
	margin-bottom: 18rpx;
}
.item_flex .item_left .desc .desc_item{
	float: left;
	color: #787878;
	font-size: 26rpx;
	background: #f1f1f1;
	border-radius: 6rpx;
	padding: 0 10rpx;
	line-height: 34rpx;
	margin: 0 15rpx 20rpx 0;
}
.item_flex .item_right .date{
	font-size: 24rpx;
	color: #999;
	line-height: 45rpx;
}
.item_flex .item_left .price_box{
	color: #fc5a1f;
	display: flex;
	align-items: flex-end;
}
.item_flex .item_left .price_box .price_num{
	font-size: 40rpx;
	position: relative;
	top: 2rpx;
}
.item_flex .item_left .price_box .price_num2{
	font-size: 34rpx;
}
.item_flex .item_left .price_box .price_unit{
	font-size: 28rpx;
}
.item_flex .item_left .special_price_icon{
	font-size: 24rpx;
	color: #f43845;
	border-radius: 6rpx;
	background: #fff0f1;
	padding: 2rpx 5rpx;
	border: 1px solid #f43845;
	margin-left: 15rpx;
	position: relative;
	top: 2rpx;
}
.item_flex .item_left .address{
	color: #888;
	font-size: 26rpx;
}
.item_flex .item_right .button{
	text-align: center;
	width: 140rpx;
	position: absolute;
	bottom: 0;
	right: 0;
	background: #ff7600;
	border-radius: 6rpx;
	color: #fffefe;
	border: none;
	font-size: 30rpx;
	line-height: 56rpx;
	height: 56rpx;
}


.end_warn,.void_wrap{
	width: 100%;
	color: #999;
	line-height: 1;
	text-align: center;
}
.end_warn{
	padding: 40rpx 0 50rpx;
}
.void_wrap{
  	padding-top: 150rpx;
}
.void_wrap image{
	width: 160rpx;
	height: 130rpx;
	display: block;
	margin: 0 auto;
}
.warnMsg_26{
	font-size: 26rpx;
	padding-bottom: 20rpx;
}
.warnMsg_30{
	font-size: 30rpx;
	padding: 25rpx;
}
.call{
	width: 210rpx;
	font-size: 28rpx;
	line-height: 54rpx;
	border: 1px solid #999;
	border-radius: 6rpx;
	margin: 0 auto;
	color: #ff7600;
	border: 1px solid #ff7600;
}
.callSet{
	margin-top: 25rpx;
}