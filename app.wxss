/**app.wxss**/
page{
	height: 100%;
	background: #f3f6f7;
	font-size: 28rpx;
}
.container {
	width: 750rpx;
	height: auto;
	display: flex;
	flex-direction: column;
	align-items: center;
	justify-content: space-between;
	box-sizing: border-box;
}
.style-placeholder{
	color: #ccc;
}
.hui{
	color: #777;
}
button{
	border-radius: 0;
}
button:after{
	display: none;
}
::-webkit-scrollbar{
	width: 0;
	height: 0;
	color: transparent;
}
